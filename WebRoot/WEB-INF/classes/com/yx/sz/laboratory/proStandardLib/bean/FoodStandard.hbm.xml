<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yx.sz.laboratory.proStandardLib.bean">
<class name="FoodStandard" table="lab_FoodStandard">
	<id name="id" type="int" column="id">
		 <generator class="native"/>
	</id>
	<property name="sequence" column="seq" type="int"/>
	<property name="name" type="string"/>
	<property name="category" type="string"/>
	<property name="standardCode" type="string"/>
	<property name="standardName" type="string"/>
	<property name="limitAndDesc" type="string"/>
	<property name="remark" type="string"/>
</class>
</hibernate-mapping>
