<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yx.sz.laboratory.workflow.bean">
<class name="CattleSampleList" table="lab_cattle_sample_list">
	<id name="id" type="long" column="id">
	     <generator class="native"/>
	</id>
	<property name="state" type="integer"/>
	<property name="userId"  type="string"/>
	<property name="userName" type="string"/>
	<property name="sampleListNum"  type="string"/>
	<property name="taskFrom" type="string"/>
	<property name="place" type="string"/>
	<property name="link" type="string"/>
	<property name="company" type="string"/>
	<property name="address" type="string"/>
	<property name="legalEntity" type="string"/>
	<property name="mobile" type="string"/>
	<property name="sampleType" type="string"/>
	<property name="foodOrquarantineNo" type="string"/>
	<property name="name" type="string"/>
	<property name="proCompany" type="string"/>
	<property name="proComAddress" type="string"/>
	<property name="quarantineNo" type="string"/>
	<property name="proDate" type="string"/>
	<property name="sampleCount" type="int"/>
	<property name="sampleBase" type="int"/>
	<property name="remark" type="string"/>
</class>
</hibernate-mapping>
